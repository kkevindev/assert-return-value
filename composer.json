{
  "name": "kkevindev/assert-return-value",
  "description": "A library that uses webmozarts/assert to assert and return the asserted value.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "webmozart/assert": "1.11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "friendsofphp/php-cs-fixer": "^3"
  },
  "autoload": {
    "psr-4": {
      "Kkevindev\\AssertReturnValue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kkevindev\\AssertReturnValue\\Tests\\": "tests/"
    }
  }
}